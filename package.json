{
  "name": "gemini-schedule-demo",
  "version": "1.0.0",
  "description": "A demo application showing how to use Google's Gemini API to create student schedules",
  "main": "dist/songrecommender-tests.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && tsc",
    "start": "npm run build && node dist/songrecommender-tests.js",
    "dev": "ts-node --transpile-only songrecommender-tests.ts",
    "test": "npm run build && node dist/songrecommender-tests.js",
    "manual": "npm run build && node -e \"require('./dist/songrecommender-tests.js').testManualRecommendation()\"",
    "llm": "npm run build && node -e \"require('./dist/songrecommender-tests.js').testLLMRecommendation()\"",
    "mixed": "npm run build && node -e \"require('./dist/songrecommender-tests.js').testMixedScheduling && require('./dist/songrecommender-tests.js').testMixedScheduling()\""
  },
  "keywords": [
    "gemini",
    "ai",
    "schedule",
    "student",
    "demo",
    "typescript"
  ],
  "author": "Student Demo",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.21.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "ts-node": "^10.9.0"
  }
}
